<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>HomePage</title>
    <style>
        .devproduct{
            width: 40px;
        }
    </style>
</head>
<body>
    <p>Ahmed mohamed</p>
    <a rel="/signup" href="/signup"><button>Signup</button></a>
    <a rel="/signin" href="/signin"><button>SignIn</button></a>  
    <a rel="/updateproduct" href="/updateproduct"><button>Update Product</button></a> 
    <a rel="/deleteproduct" href="/deleteproduct"><button>Delete Product</button></a> 
    <a rel="/addproduct" href="/addproduct"><button>Add Product</button></a> 
    <a rel="/cart" href="/cart"><button>cart</button></a>

    <h1>Welcome to the Homepage</h1>
    
    <ul>
        <% item.forEach(product => { %>
            <li><%= product.title %><dev class="devproduct"></dev><%= product.price %> <a href="/addcart/<%=product._id%>"><button>addtocart</button></a><dev class="devproduct"></dev><a href="/review/<%=product._id%>"><button>addreview</button></a></li>
        <% }); %>
    </ul>
    
</body>
</html>