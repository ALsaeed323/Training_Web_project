        <% productsData.forEach(product => { %>
            <li><%= product.title %><dev class="devproduct"></dev><%= product.price %> <a href="/confirm/<%=product._id%>"><button>confirm</button></a></li>
        <% }); %>
    </ul>

   <a href="/displayorders"><button>Orders</button></a>
</body>
</html>